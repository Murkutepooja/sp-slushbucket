<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sp_widget"&gt;&lt;sp_widget action="INSERT_OR_UPDATE"&gt;&lt;category&gt;custom&lt;/category&gt;&lt;client_script&gt;&lt;![CDATA[function($scope) {
  var c = this;
	var g_form = $scope.page.g_form;
	c.data.selected = [];
	c.data.stage = [];
	c.leftFilter = '';//for input ng-model
	c.rightFilter = '';//for input ng-model
	
	c.select = function(rec,from,event){
		console.log(event);
		if(!event.shiftKey &amp;&amp; !event.ctrlKey){
			from = _.map(from,function(x){x.selected = x==rec})
		}
		if(event.ctrlKey &amp;&amp; !event.shiftKey){
			rec.selected = !rec.selected;
		}
		if(event.shiftKey &amp;&amp; !event.ctrlKey){
			var lri = from.indexOf(c.data.lastRec);
			var cri = from.indexOf(rec);
			if(lri==cri||lri==-1){return;}
			var start = (lri&lt;cri) ? lri : cri;
			var end = (lri&lt;cri) ? cri : lri;
			from = _.map(from,function(x,i){x.selected = (i&gt;=start &amp;&amp; i&lt;=end)||x.selected})
		}
		c.data.lastRec = rec;
	}
	
	c.selectandMove = function(rec,from){
		var direct = (from==c.data.recs)?'right':'left';
		from = _.map(from,function(x){x.selected = x==rec});
		c.move(direct);
	}
	
	//replace direction with to, from, stage
	c.move = function(direct){
		var right = direct=='right';
		var to = (right)?c.data.selected:c.data.recs;
		var from = (right)?c.data.recs:c.data.selected;
		var stage = _.filter(from,function(x){return x.selected==true;});
		c.data.stage = stage;
		to = _.concat(to,stage);
		to = _.map(to,deselect);
		to = _.sortBy(to,[sortArr]);
		from = _.filter(from, filterOutMatch);
		c.data.recs = (right)?from:to;
		c.data.selected=(right)?to:from;
		g_form.setValue(c.data.variableName,_.map(c.data.selected,fillValue),_.map(c.data.selected,fillDispValue));
	}
	
	function deselect(x){
		x.selected = false;
		return x;
	}
	
	function sortArr(x){
		return x[c.data.sortBy];
	}
	
	function filterOutMatch(x){
		return c.data.stage.indexOf(x)==-1;
	}
	
	function fillValue(x){
		return x.sys_id;
	}
	
	function fillDispValue(x){
		return x[c.data.display];
	}
}]]&gt;&lt;/client_script&gt;&lt;controller_as&gt;c&lt;/controller_as&gt;&lt;css&gt;.slush {
  display:flex
}

.bucket-wrapper {
  flex:1;
  input{
    margin-bottom: 0.5rem; 
  }
}

.bucket {
  height:300px;
  overflow-y:scroll;	
  border:1px solid #ddd;
}

.middle {
  flex:0.5 ;
  display: flex;
  align-items:center;
  justify-content:space-evenly;
}

.selected {
 	background-color:lightyellow; 
}

.list-group-item.thin {
 	padding: 3px; 
  border-radius:0;
  -webkit-user-select: none;
-khtml-user-select: none;
-moz-user-select: none;
-o-user-select: none;
user-select: none;
}
&lt;/css&gt;&lt;data_table&gt;sp_instance&lt;/data_table&gt;&lt;demo_data/&gt;&lt;description&gt;A slushbucket widget for the service portal&lt;/description&gt;&lt;docs/&gt;&lt;field_list/&gt;&lt;has_preview&gt;true&lt;/has_preview&gt;&lt;id&gt;sp_slushbucket&lt;/id&gt;&lt;internal&gt;false&lt;/internal&gt;&lt;link&gt;&lt;![CDATA[function link(scope, element, attrs, controller) {
  
}]]&gt;&lt;/link&gt;&lt;name&gt;SP Slushbucket&lt;/name&gt;&lt;option_schema/&gt;&lt;public&gt;false&lt;/public&gt;&lt;roles/&gt;&lt;script&gt;&lt;![CDATA[(function() {
	data.variableName = options.variableName;
	data.table = options.table||'No Table defined in options';
	data.query = options.refQual;
	data.sortBy = options.sortBy;
	data.display = options.display||'sys_id';
	data.secondaryField = options.secondaryField;
	data.fields = options.fields||'getDisplay';
	data.recs = [];
	if(data.table=='No Table defined in options'){return}
	var gr = new GlideRecord(data.table);
	if(data.query){gr.addEncodedQuery(data.query);}
	if(data.sortBy){gr.orderBy(data.sortBy);}
	gr.query();
	while(gr.next()){
		var obj = {
			selected:false,
			sys_id:gr.sys_id.toString()
		};
		addFields(gr,obj);
		data.recs.push(obj);
	}
	
	function addFields(gr,obj){
		if(data.fields=='getDisplay'){
			obj[gr.getDisplayName()] = gr.getDisplayValue();
			return obj;
		} else {
			for(var i in data.fields){
				obj[data.fields[i]] = gr.getValue(data.fields[i]);
			}
			return obj;
		}
	}
	
})();]]&gt;&lt;/script&gt;&lt;servicenow&gt;false&lt;/servicenow&gt;&lt;sys_class_name&gt;sp_widget&lt;/sys_class_name&gt;&lt;sys_created_by&gt;admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2020-03-27 19:14:35&lt;/sys_created_on&gt;&lt;sys_id&gt;a1a22efd1bf74010c64c31d7dc4bcb8f&lt;/sys_id&gt;&lt;sys_mod_count&gt;167&lt;/sys_mod_count&gt;&lt;sys_name&gt;SP Slushbucket&lt;/sys_name&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sp_widget_a1a22efd1bf74010c64c31d7dc4bcb8f&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;admin&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2020-03-30 07:40:02&lt;/sys_updated_on&gt;&lt;template&gt;&lt;![CDATA[&lt;div ng-if="c.data.table=='No Table defined in options'"class="alert alert-danger"&gt;
  &lt;i class="fa fa-info-circle"&gt;&lt;/i&gt; No Table defined in options
&lt;/div&gt;
&lt;div class="slush"&gt;
  &lt;div class="bucket-wrapper"&gt;
    &lt;input type="text" class="form-control" ng-model="c.leftFilter"&gt;
    &lt;div class="left bucket" &gt;
      &lt;ul class="list-group"&gt;
        &lt;li title="{{rec[c.data.secondaryField]}}" 
            ng-repeat="rec in c.data.recs | filter: c.leftFilter track by $index" 
            ng-click="c.select(rec,c.data.recs,$event)"
            ng-dblclick="c.selectandMove(rec,c.data.recs,$event)"
            ng-class="{selected:rec.selected}" 
            class="list-group-item thin"&gt;
          {{rec[c.data.display]}} 
        &lt;/li&gt;
      &lt;/ul&gt;
    &lt;/div&gt;
  &lt;/div&gt;
  &lt;div class="middle"&gt;
    &lt;div ng-click="c.move('right')" class="btn btn-default"&gt;
      &lt;i class="fa fa-chevron-right"&gt;&lt;/i&gt;
    &lt;/div&gt;
    &lt;div ng-click="c.move('left')" class="btn btn-default"&gt;
      &lt;i class="fa fa-chevron-left"&gt;&lt;/i&gt;
    &lt;/div&gt;
  &lt;/div&gt;  
  &lt;div class="bucket-wrapper"&gt;
    &lt;input type="text" class="form-control" ng-model="c.rightFilter"&gt;
    &lt;div class="right bucket"&gt;
    	&lt;ul class="list-group"&gt;
        &lt;li title="{{rec[c.data.secondaryField]}}"  
            ng-repeat="rec in c.data.selected | filter: c.rightFilter track by $index" 
            class="list-group-item thin" 
            ng-class="{selected:rec.selected}" 
            ng-click="c.select(rec,c.data.selected,$event)"
            ng-dblclick="c.selectandMove(rec,c.data.selected,$event)"&gt;
          {{rec[c.data.display]}}
        &lt;/li&gt;
      &lt;/ul&gt;
    &lt;/div&gt;
  &lt;/div&gt;
&lt;/div&gt;]]&gt;&lt;/template&gt;&lt;/sp_widget&gt;&lt;/record_update&gt;</payload>
